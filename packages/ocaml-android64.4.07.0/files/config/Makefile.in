BYTECC=%{conf-android:toolchain}%/%{conf-android:triplesub}%%{conf-android:level}%-clang %{conf-android:cflags}% %{conf-android:ldflags}% -O2 -fPIC
CC=%{conf-android:toolchain}%/%{conf-android:triplesub}%%{conf-android:level}%-clang %{conf-android:cflags}% %{conf-android:ldflags}%
BYTECCCOMPOPTS=-fno-defer-pop -Wall -D_FILE_OFFSET_BITS=64 -D_REENTRANT
